<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.fatec.telas.MenuInterface">
   <children>
      <MenuBar>
        <menus>
          <Menu fx:id="userMenu" mnemonicParsing="false" text="Usuário">
            <items>
              <MenuItem fx:id="insertUserMenuItem" mnemonicParsing="false" text="Inserir Usuário" />
                  <MenuItem fx:id="listUserMenuItem" mnemonicParsing="false" text="Listar Usuário" />
            </items>
          </Menu>
          <Menu fx:id="clientMenu" mnemonicParsing="false" text="Cliente">
            <items>
              <MenuItem fx:id="insertClientMenuItem" mnemonicParsing="false" text="Inserir Cliente" />
                  <MenuItem fx:id="listClientMenuItem" mnemonicParsing="false" text="Listar Cliente" />
            </items>
          </Menu>
          <Menu fx:id="petMenu" mnemonicParsing="false" text="Pet">
            <items>
              <MenuItem fx:id="insertPetMenuItem" mnemonicParsing="false" text="Inserir Pet" />
                  <MenuItem fx:id="listPetMenuItem" mnemonicParsing="false" text="Listar Pet" />
            </items>
          </Menu>
          <Menu fx:id="clientPetMenu" mnemonicParsing="false" text="Pet - Cliente">
            <items>
              <MenuItem fx:id="insertClientPetMenuItem" mnemonicParsing="false" text="Relacionar Pet e Cliente" />
                  <MenuItem fx:id="listClientPetMenuItem" mnemonicParsing="false" text="Listar relações" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
